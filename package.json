  },
  "devDependencies": {
    "babel-jest": "^26.0.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.0",
    "jest": "^26.0.1",
    "microbundle": "^0.12.0",
    "preact": "^10.4.4"
  },
  "dependencies": {
    "unifire": "^0.3.0"
  }
