{
  "name": "@unifire/preact",
  "version": "0.2.0",
  "description": "Preact bindings for Unifire",
  "main": "dist/unifire-preact.js",
  "module": "dist/unifire-preact.module.js",
  "umd:main": "dist/unifire-preact.umd.js",
  "unpkg": "dist/unifire-preact.umd.js",
